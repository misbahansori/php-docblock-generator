{
  "name": "php-docblock-generator",
  "displayName": "php-docblock-generator",
  "description": "Generate php docblock",
  "publisher": "misbahansori",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.56.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:php-docblock-generator.createDocBlock"
  ],
  "main": "./src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "php-docblock-generator.createDocBlock",
        "title": "Create DocBlock"
      }
    ],
    "keybindings": [
      {
          "command": "php-docblock-generator.createDocBlock",
          "key": "ctrl+shift+c",
          "mac": "cmd+shift+c",
          "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.56.0",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  },
  "dependencies": {
    "node-natural-sort": "^0.8.7"
  }
}
